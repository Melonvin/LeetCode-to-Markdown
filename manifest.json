{
  "manifest_version": 2,
  "name": "LeetCode to Markdown",
  "description": "This extension demonstrates a browser action with kittens.",
  "version": "1.0",
  "permissions": [
    "https://leetcode-cn.com/*"
  ],
  "browser_action": {
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://leetcode-cn.com/*"
      ],
      "js": [
        "script.js"
      ]
    }
  ]
}